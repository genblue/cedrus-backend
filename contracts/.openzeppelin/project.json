{
  "manifestVersion": "2.2",
  "contracts": {
    "CedrusToken": "CedrusToken"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.5.0"
  },
  "name": "cedar",
  "version": "1.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": true,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": false
    },
    "manager": "openzeppelin",
    "solcVersion": "0.5.17",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts"
  },
  "telemetryOptIn": true
}
